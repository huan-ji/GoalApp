<h1>Sign In</h1>
<%= flash[:errors] %>
<form class="" action="/session" method="post">
  <%= auth_token %>
  <label>Username
    <input type="text" name="user[username]" value="">
  </label>
  <br>

  <label>Password
    <input type="password" name="user[password]" value="">
  </label>
  <br>

  <input type="submit" value="Sign In">
</form>
